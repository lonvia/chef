<%= node[:nominatim][:logdir] %>/update.log {
  weekly
  missingok
  rotate 30
  compress
  delaycompress
  notifempty
  create 640 nominatim adm
}

<%= node[:nominatim][:logdir] %>/query.log {
  weekly
  missingok
  rotate 8
  compress
  delaycompress
  notifempty
  postrotate
        systemctl reload nominatim.service
  endscript
  create 640 www-data adm
}
